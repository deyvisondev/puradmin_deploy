<%- include("partials/header") %>

  <style>
    #frm-add-category label.error {
      color: red;
    }
  </style>

  <!-- Content Header (Page header) -->
  <div class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1 class="m-0">Buscar Desempenho de um Picker</h1>
        </div><!-- /.col -->
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="/admin/">In√≠cio</a></li>
            <li class="breadcrumb-item active">Buscar Desempenho de um Picker</li>
          </ol>
        </div><!-- /.col -->
      </div><!-- /.row -->
    </div><!-- /.container-fluid -->
  </div>
  <!-- /.content-header -->

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <!-- Small boxes (Stat box) -->
      <div class="row">
        <div class="col-md-12">
          <div class="card card-dark">
            <div class="card-header">
              <h3 class="card-title">Buscar Desempenho de um Picker</h3>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
              <div class="row">
                <div class="col-md-6">
                  <div class="input-group">
                    <form action="/admin/picker-dashboard" method="GET">
                      <label for="date-billing">Picker:</label>
                      <select required class="select2" data-placeholder="Escolha o picker" style="width: 100%;"
                        name="user">
                        <% users.forEach(function(user){ %>
                          <option></option>
                          <option value="<%= user.id %>">
                            <%= user.name %>
                          </option>
                          <% }) %>
                      </select>
                  </div>
                  <!-- /.input group -->
                </div>
              </div>
              <button type="submit">Buscar</button>
              </form>

            </div>
            <!-- /.card-body -->
          </div>
        </div>
      </div>
      <!-- /.row -->
    </div><!-- /.container-fluid -->

  </section>
  <!-- /.content -->
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>




  <%- include("partials/footer") %>